    .ZiXun-modal-cancel {
      position: absolute;
      z-index: 2000;
      top: 0;
      right: 0;
      bottom: 0;
      left: 0;
      background: rgba(0, 0, 0, 0.5);
    }    
    .ZiXun-bottom-dialog-body {
      width: 100%;
      position: absolute;
      z-index: 3000;
      bottom: 0;
      left: 0;
      background: #fff;
      padding: 32rpx 32rpx 46rpx;
      box-sizing: border-box;
      border-radius: 28rpx 28rpx 0 0;
    }    
    .ZiXun-bottom-dialog-body.isIPX {
      padding-bottom: 72rpx;
    }  
