  box-sizing: border-box;
}

.list-notify .item .title image {
  width: 32rpx;
  height: 32rpx;
  position: absolute;
  top: 0;
  left: 0;
}

.list-notify .item .title text {
  display: block;
  font-size: 28rpx;
  padding-left: 40rpx;
  color: #333;
  line-height: 40rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 3;
  -webkit-box-orient: vertical;
}

.list-notify  .item .t-c {
  display: flex;
  justify-content: space-between;
  font-size: 24rpx;
  line-height: 40rpx;
  text-align: right;
  height: 40rpx;
  margin-top: 20rpx;
  /* position: absolute;
  bottom: 0;
  right: 0; */
}

.list-notify .item .t-c image {
  width: 32rpx;
  height: 32rpx;
  margin-top: 4rpx;
  display: block;
  float: left;
}

.list-notify  .item  .t-c text {
  padding: 0 10rpx;
  display: block;
  float: left;
  color: #999;
}

.list-notify2 .date {
  font-size: 22rpx;
  color: #999;
  line-height: 60rpx;
  text-align: center;
}
.list-notify2-contentBox {
  margin: 0 0rpx;
}

.list-notify2-contentBox .title {
