.swiper-box1 .lcxq .table1 .td.tt {
  color: #333;
  width: 35%;
}

.swiper-box1 .lcxq .list3 {
  margin-bottom: 10rpx;
}

.swiper-box1 .lcxq .list3 .title3 {
  line-height: 80rpx;
  height: 80rpx;
  border-bottom: 2rpx solid #eee;
  font-size: 28rpx;
  color: #333;
  padding: 0 20rpx;
  background: #f5f5f5;
}

.swiper-box1 .lcxq .list3 .title3 image {
  width: 50rpx;
  height: 50rpx;
  float: right;
  margin-top: 15rpx;
}

.swiper-box1 .lcxq .list3 .con3 {
  font-size: 24rpx;
  line-height: 40rpx;
  color: #999;
}

.swiper-box1 .lcxq .list3 .con3 .table {
  padding: 20rpx;
}

.swiper-box1 .lcxq .list3 .con3 .table3a .tr,
.swiper-box1 .lcxq .list3 .con3 .table3b .tr {
  display: flex;
  flex-wrap: wrap;
