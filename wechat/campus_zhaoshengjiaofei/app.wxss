}

.swiper-box1 .lcxq .list1 {
  margin-bottom: 10rpx;
}

.swiper-box1 .lcxq .list1 .title1 {
  line-height: 80rpx;
  height: 80rpx;
  border-bottom: 2rpx solid #eee;
  font-size: 28rpx;
