}

.swiper-box1 .lcxq .list3 .con3 .table3a .tr .td {
  width: 80%;
  line-height: 40rpx;
  padding: 10rpx;
  box-sizing: border-box;
  border-right: 2rpx solid #eee;
}

.swiper-box1 .lcxq .list3 .con3 .table3a .tr .td.tt {
  color: #333;
  width: 20%;
  box-sizing: border-box;
  background: #f5f5f5;
  line-height: 40rpx;
  padding: 10rpx;
}

