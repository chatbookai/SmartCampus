/**资讯**/

.list-news {
  background: #fff;
  width: 100%;
  height: auto;
  overflow: hidden;
  box-sizing: border-box;
  background: #f8f8f8;
}

.list-news .itemx {
  background: #fff;
  margin-bottom: 20rpx;
  padding: 20rpx;
  border-radius: 10rpx;
}

.list-news .item {
  width: 100%;
  height: auto;
  overflow: hidden;
  display: flex;
  justify-content: space-around;
}

.list-news .item .img {
  width: 220rpx;
  height: 150rpx;
  border-radius: 6rpx;
  overflow: hidden;
  position: relative;
}

.list-news .item .img .nav {
  position: absolute;
  top: 6rpx;
  left: 6rpx;
  background: rgba(0, 0, 0, 0.5);
  font-size: 20rpx;
  color: #fff;
  width: 120rpx;
  line-height: 40rpx;
  height: 40rpx;
  text-align: center;
  border-radius: 6rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.list-news .item .img image {
  width: 100%;
  height: 100%;
}

.list-news .item .con {
  width: 1rpx;
  flex: 1;
  padding-left: 18rpx;
  position: relative;
}

.list-news .item .con .title {
  font-size: 28rpx;
  color: #333;
  line-height: 40rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  font-weight: bold;
}

.list-news .item .con .content {
    font-size: 24rpx;
    color: #333;
    line-height: 40rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
  }


.list-news .item .con .t-c {
  display: flex;
  justify-content: space-between;
  font-size: 24rpx;
}

.list-news .item .con .t-c image {
  width: 32rpx;
  height: 32rpx;
  margin-top: 4rpx;
  display: block;
  float: left;
}

.list-news .item .con text {
  padding: 0 10rpx;
  display: block;
  float: left;
  color: #999;
}

.list-notify {
  background: #f8f8f8;
  width: 100%;
  height: auto;
  overflow: hidden;
  box-sizing: border-box;
}

.list-notify .itemx {
  margin-bottom: 20rpx;
